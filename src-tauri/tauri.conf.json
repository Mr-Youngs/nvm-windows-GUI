{
    "productName": "nvm-windows GUI",
    "version": "0.4.0",
    "identifier": "com.nvmwindowsgui.manager",
    "build": {
        "beforeBuildCommand": "npm run build",
        "beforeDevCommand": "npm run dev",
        "devUrl": "http://localhost:8080",
        "frontendDist": "../dist"
    },
    "app": {
        "windows": [
            {
                "label": "main",
                "fullscreen": false,
                "height": 720,
                "resizable": true,
                "title": "nvm-windows GUI",
                "width": 1024
            }
        ]
    },
    "bundle": {
        "active": true,
        "category": "DeveloperTool",
        "copyright": "",
        "externalBin": [],
        "icon": [
            "icons/icon.png",
            "icons/icon.ico"
        ],
        "longDescription": "",
        "shortDescription": "",
        "targets": "all"
    }
}